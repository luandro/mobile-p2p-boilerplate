{
  "name": "ssb-graphql",
  "version": "0.0.0",
  "description": "GraphQL server for Scuttlebot",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "preserve": "babel src -d dist -x .js,.graphql --presets es2015,stage-2 --plugins babel-plugin-inline-import,transform-object-rest-spread",
    "start": "nodemon src --exec babel-node",
    "serve": "node dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "body-parser": "1.18.2",
    "express": "4.16.2",
    "express-graphql": "0.6.11",
    "graphql": "0.12.3",
    "graphql-tools": "2.18.0",
    "pull-stream": "3.6.1",
    "ssb-party": "0.5.1",
    "ssb-ref": "2.9.1"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-plugin-inline-import": "2.0.6",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "nodemon": "1.14.10"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "babel-plugin-inline-import",
      "transform-object-rest-spread"
    ]
  }
}
