{
  "name": "rnnodeapp",
  "version": "0.0.0",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run build",
    "build": "babel src --out-dir lib",
    "watch": "babel src --out-dir lib --watch",
    "start": "concurrently \"npm run watch\" \"nodemon index.js\""
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.42",
    "bufferutil": "3.0.1",
    "dat-node": "^3.5.8",
    "graphql-yoga": "^1.5.2",
    "leveldown-android-prebuilt": "2.0.3",
    "patchcore": "1.23.3",
    "promise": "^8.0.1",
    "pull-stream": "^3.6.2",
    "scuttlebot": "10.5.0",
    "sodium-browserify-tweetnacl": "0.2.3",
    "ssb-about": "0.1.x",
    "ssb-backlinks": "0.4.x",
    "ssb-blobs": "1.1.4",
    "ssb-client": "^4.5.7",
    "ssb-contacts": "0.0.x",
    "ssb-friends": "^2.3.0",
    "ssb-keys": "7.0.12",
    "ssb-party": "^0.5.1",
    "ssb-private": "0.1.x",
    "ssb-query": "0.1.x",
    "ssb-ref": "^2.9.1",
    "ssb-serve-blobs": "1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.42",
    "@babel/core": "^7.0.0-beta.42",
    "@babel/helper-plugin-utils": "^7.0.0-beta.42",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.42",
    "@babel/plugin-transform-async-to-generator": "^7.0.0-beta.42",
    "@babel/preset-env": "^7.0.0-beta.42",
    "concurrently": "^3.5.1",
    "nodemon": "^1.17.2"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": [
              "last 2 Chrome versions"
            ]
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-transform-async-to-generator"
    ]
  }
}
